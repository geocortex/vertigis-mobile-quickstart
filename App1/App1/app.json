    {
      "$type": "menu-item",
      "id": "rotateMapAction",
      "title": "Rotate Map",
      "isEnabled": true,
      "action": [
        {
          "name": "panel.snap-host-panel",
          "arguments": {
            "snapPosition": "middle"
          }
        },
        {
          "name": "map.rotate-by",
          "arguments": {
            "rotation": 45,
            "maps": "item://map-extension/map1"
